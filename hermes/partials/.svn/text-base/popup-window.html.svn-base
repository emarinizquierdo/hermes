<!-- Modal -->
<div id="popupModal" class="modal hide" tabindex="-1" role="dialog" aria-labelledby="popupModalLabel" aria-hidden="true" >
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="popupModalLabel" >{{alert.title}}</h3>
  </div>
  <div class="modal-body">
    <p>{{alert.message}}</p>
  </div>
  <div class="modal-footer">
    <button class="btn" ng-click="alert.toDo()" ng-show="alert.showAccept" data-dismiss="modal">{{language.ACCEPT}}</button>
    <button class="btn btn-primary" aria-hidden="true" data-dismiss="modal" >{{alert.cancelButton}}</button>
  </div>
</div>