div.speech
{
	position: absolute;
	margin-left: 275px;
	margin-top: -46px;
	padding-top: 7px;
	width: 250px;
	height: 93px;
	text-align: center;
	background-color: #fff;
	border: 8px solid rgb(77, 77, 77);;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	border-radius: 30px;
	-webkit-box-shadow: 2px 2px 4px #888;
	-moz-box-shadow: 2px 2px 4px #888;
	box-shadow: 2px 2px 4px #888;
}

div.speech:before
{
	content: ' ';
	position: absolute;
	width: 0;
	height: 0px;
	left: 250px;
	top: 40px;
	border: 25px solid;
	border-color: transparent transparent rgb(77, 77, 77) rgb(77, 77, 77);
}

div.speech:after
{
	content: ' ';
	position: absolute;
	width: 0;
	height: 0;
	left: 250px;
	top: 51px;
	border: 15px solid;
	border-color: transparent transparent #fff #fff;
}